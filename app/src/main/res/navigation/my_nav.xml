<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/berandaFragment">

    <fragment
        android:id="@+id/newsDetailsFragment"
        android:name="com.kominfo.disabilityinformationsystem.NewsDetailsFragment"
        android:label="News Details"
        tools:layout="@layout/fragment_news_details" />
    <fragment
        android:id="@+id/berandaFragment"
        android:name="com.kominfo.disabilityinformationsystem.BerandaFragment"
        android:label="Beranda"
        tools:layout="@layout/fragment_beranda" >
        <action
            android:id="@+id/action_berandaFragment_to_layananDukcapilFragment"
            app:destination="@id/layananDukcapilFragment" />
    </fragment>
    <fragment
        android:id="@+id/newsFragment"
        android:name="com.kominfo.disabilityinformationsystem.NewsFragment"
        android:label="News"
        tools:layout="@layout/fragment_news" />
    <fragment
        android:id="@+id/twitterFragment"
        android:name="com.kominfo.disabilityinformationsystem.TwitterFragment"
        android:label="Twitter"
        tools:layout="@layout/fragment_twitter" />
    <fragment
        android:id="@+id/layananDukcapilFragment"
        android:name="com.kominfo.disabilityinformationsystem.dukcapil.LayananDukcapilFragment"
        android:label="fragment_layanan_dukcapil"
        tools:layout="@layout/fragment_layanan_dukcapil" >
        <action
            android:id="@+id/action_layananDukcapilFragment_to_aktaKelahiranFragment"
            app:destination="@id/aktaKelahiranFragment" />
        <action
            android:id="@+id/action_layananDukcapilFragment_to_gantiNamaFragment"
            app:destination="@id/gantiNamaFragment" />
    </fragment>
    <fragment
        android:id="@+id/aktaKelahiranFragment"
        android:name="com.kominfo.disabilityinformationsystem.dukcapil.AktaKelahiranFragment"
        android:label="fragment_akta_kelahiran"
        tools:layout="@layout/fragment_akta_kelahiran" />
    <fragment
        android:id="@+id/gantiNamaFragment"
        android:name="com.kominfo.disabilityinformationsystem.dukcapil.GantiNamaFragment"
        android:label="fragment_ganti_nama"
        tools:layout="@layout/fragment_ganti_nama" />
</navigation>